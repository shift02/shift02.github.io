<!DOCTYPE html>
<html>
<head>
<%
var data = jsonData;
%>
<%- include('./common/_head'); %>
<link rel="stylesheet" href="./css/index.css">
<title><%- data.title %></title>
</head>
<body>

  <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <%- include('./common/_header'); %>
    <%- include('./common/_drawer'); %>
    <main class="mdl-layout__content">

      <section id="mods">

        <div class="mods-title mdl-typography--display-1-color-contrast">Mods</div>
        <div class="mdl-grid">
          <% data.mods.forEach(function(mod){ %>
          <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mods-card-square mdl-card mdl-shadow--2dp">
            <div class="mdl-card__title mdl-card--expand"
            style="background: url(<%- mod.img %>) bottom right 15% no-repeat  #46B6AC;background-size: cover;">
              <h2 class="mdl-card__title-text"><%- mod.name %></h2>
            </div>
            <div class="mdl-card__supporting-text">
            <span class="mdl-chip"> <span
                class="mdl-chip__text"><%- mod.version %></span>
              </span>
            </div>
            <div class="mdl-card__supporting-text">
              <%- mod.description %>
            </div>
            <div class="mdl-card__actions mdl-card--border">
              <a
                class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
                詳細 </a>
            </div>
          </div>
          <% }) %>
        </div>
      </section>

      <section id="web_game">

        <div class="mods-title mdl-typography--display-1-color-contrast">Web Game</div>

      </section>

    </main>
  </div>

</body>
</html>